<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.smarthaccp.demand.mapper.HandlerMapper" >
	<select id="selectHandle" parameterType="String" resultType="HandlerVO">
		/* selectHandle */
		SELECT
			*
		FROM
			REQ_HANDLE_TB
		WHERE
			handle_idx = #{handle_idx}
	</select>
	<insert id="insertHandle" parameterType="HandlerVO">
		INSERT INTO
			REQ_HANDLE_TB(handle_idx,req_idx,handle_contents,handler,file_idx)
			values(
				#{handle_idx}
				, #{req_idx}
				, #{handle_contents}
				, #{handler}
				, #{file_idx}
			)
	</insert>
</mapper>
